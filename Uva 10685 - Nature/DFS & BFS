using namespace std;
#include<bits/stdc++.h>
#define ll long long
#define ull unsigned long long
#define Case int t ; cin>>t ; while(t--)
#define all(v) ((v).begin()),((v).end())
#define sz(n) (int)n.size()
#define fx(x) fixed<<setprecision(x)
#define prl pair<ll , ll>
#define pri pair<int , int>
#define pb push_back
#define f first
#define s second
ll mod = 1e9 + 7 ;
int dx[] = {0,0,1,1,1,-1,-1,-1};
int dy[] = {-1,1,-1,0,1,-1,0,1};
void file(){
#ifndef ONLINE_JUDGE
    freopen("input.txt", "r", stdin);
    freopen("output.txt", "w", stdout);
#endif
}
void fast (){
    ios_base::sync_with_stdio(0);
    cin.tie(0);
}
const int N = 1e6+5 ;
int n , m ;
vector<int> adj[N] ;
vector<int> vis(N) ;
map<string , int> mp ;
int ans = 0 ;
int dfs (int p){
    int ans = 1 ;
    vis[p] = true ;
    for(auto ch : adj[p])if(!vis[ch]) ans+=dfs(ch);
    return ans ;
}
void solve() {
    while(cin >> n >> m && (m||n)){
        mp.clear() ;
        for(int i = 0 ; i<n ; i++) vis[i]=0,adj[i].clear() ;
        for(int i = 0 ; i<n ; i++){
            string s ; cin>>s ;
            mp[s] = i ;
        }
        while(m--){
            string s1 , s2 ; cin>>s1>>s2 ;
            int u = mp[s1] , v = mp[s2] ;
            adj[u].push_back(v);
            adj[v].push_back(u);
        }
        int res = 1 ;
        for(int i = 0 ; i<n ; i++){
            if(!vis[i]) res = max(res , dfs(i)) ;
        }
        cout << res << "\n";
    }
}
int main(){
    fast() ;
    file();
    solve();
}
